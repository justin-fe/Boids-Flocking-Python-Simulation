   "source": [
    "# Calculate and plot average distance to center of mass (Cohesion) over time.\n",
    "\n",
    "avg_distances = []\n",
    "for boids, _ in positions_history:\n",
    "    center = np.mean(boids, axis=0)\n",
    "    distances = np.linalg.norm(boids - center, axis=1)\n",
    "    avg_distances.append(np.mean(distances))\n",
    "\n",
    "plt.figure(figsize=(8, 4))\n",
    "plt.plot(avg_distances)\n",
    "plt.xlabel(\"Time Step\")\n",
    "plt.ylabel(\"Average Distance to Center (Cohesion)\")\n",
    "plt.title(\"Cohesion Over Time\")\n",
    "plt.grid(True)\n",
    "plt.show()\n",
    "\n",
    "# Dispersion can also be analyzed by looking at variance of positions.\n",
    "variances = []\n",
    "for boids, _ in positions_history:\n",
    "    variances.append(np.var(boids))\n",
    "\n",
    "plt.figure(figsize=(8, 4))\n",
    "plt.plot(variances)\n",
    "plt.xlabel(\"Time Step\")\n",
    "plt.ylabel(\"Position Variance (Dispersion)\")\n",
    "plt.title(\"Dispersion Over Time\")\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
